# !/usr/bin/env bash
# start

source $CLUSTERDB/venv/bin/activate
export FLASK_APP=cluster
export FLASK_ENV=development

# Foreground
flask run -h $CLUSTERDB_HOST -p $CLUSTERDB_PORT

